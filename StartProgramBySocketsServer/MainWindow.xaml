<Window x:Class="StartProgramBySocketsServer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StartProgramBySocketsServer"
        mc:Ignorable="d"
        Title="Server" Height="350" Width="525">
    <Grid>
        <Label x:Name="label" Content="Client" HorizontalAlignment="Left" Margin="100,63,0,0" VerticalAlignment="Top" RenderTransformOrigin="-3.5,0.308"/>
        <Label x:Name="label_Copy" Content="Program" HorizontalAlignment="Left" Margin="100,122,0,0" VerticalAlignment="Top" RenderTransformOrigin="-3.5,0.308"/>
        <Button x:Name="buttonStartStopProgram" Content="Start" HorizontalAlignment="Left" Height="32" Margin="283,234,0,0" VerticalAlignment="Top" Width="107" Click="buttonStartStopProgram_Click"/>
        <ComboBox x:Name="comboBoxChooseClient" HorizontalAlignment="Left" Margin="283,67,0,0" VerticalAlignment="Top" Width="120"/>
        <ComboBox x:Name="comboBoxChooseProgram" HorizontalAlignment="Left" Margin="283,126,0,0" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
