<Window x:Class="StartProgramBySocketsServer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StartProgramBySocketsServer"
        mc:Ignorable="d"
        Title="Server" Height="237" Width="351" Closing="Window_Closing" ResizeMode="CanMinimize">
    <Grid>
        <Label x:Name="label" Content="Client" HorizontalAlignment="Left" Margin="37,32,0,0" VerticalAlignment="Top" RenderTransformOrigin="-3.5,0.308"/>
        <Label x:Name="label_Copy" Content="Program" HorizontalAlignment="Left" Margin="37,78,0,0" VerticalAlignment="Top" RenderTransformOrigin="-3.5,0.308"/>
        <Button x:Name="buttonStartStopProgram" Content="Start/Stop" HorizontalAlignment="Left" Height="32" Margin="135,143,0,0" VerticalAlignment="Top" Width="107" Click="buttonStartStopProgram_Click"/>
        <ComboBox x:Name="comboBoxChooseClient" HorizontalAlignment="Left" Margin="135,36,0,0" VerticalAlignment="Top" Width="152" SelectionChanged="comboBoxChooseClient_SelectionChanged"/>
        <ComboBox x:Name="comboBoxChooseProgram" HorizontalAlignment="Left" Margin="135,82,0,0" VerticalAlignment="Top" Width="152"/>

    </Grid>
</Window>
